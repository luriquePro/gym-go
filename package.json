{
  "name": "gym-go",
  "version": "1.0.0",
  "description": "Aplicação GymGo para gerenciamento de check-ins em academias.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src/server.ts --format cjs --dts --clean",
    "start": "node dist/server.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky"
  },
  "keywords": [
    "gym-go",
    "gym",
    "fitness",
    "checkin",
    "academia",
    "gym-management"
  ],
  "author": "Luiz Henrique",
  "license": "ISC",
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.36.0",
    "@types/node": "24.5.2",
    "eslint": "9.36.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "9.1.7",
    "prettier": "3.6.2",
    "prisma": "6.16.2",
    "tsup": "8.5.0",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "typescript-eslint": "8.44.1"
  },
  "dependencies": {
    "@prisma/client": "6.16.2",
    "dotenv": "17.2.2",
    "fastify": "5.6.1",
    "zod": "4.1.11"
  }
}
